<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_product">

    <fragment
        android:id="@+id/nav_product"
        android:name="com.example.b2bcamp.ui.home.HomeFragment"
        android:label= "Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_myorders"
        android:name="com.example.b2bcamp.ui.myorders_seller.MySellerOrderFragment"
        android:label="My Orders"
        tools:layout="@layout/fragment_my_seller_order" />

    <fragment
        android:id="@+id/nav_inquiries"
        android:name="com.example.b2bcamp.ui.myinquiry.MyInquiry"
