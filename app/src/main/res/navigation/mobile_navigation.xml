    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.b2bcamp.ui.profile.MyProfileFragment"
        android:label="My Profile"
        tools:layout="@layout/fragment_my_profile" />

    <fragment
        android:id="@+id/nav_notification"
        android:name="com.example.b2bcamp.ui.notification.NotificationFragment"
        android:label="Notifications"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/nav_chats"
        android:name="com.example.b2bcamp.ui.sellerchatlist.SellerChatListFragment"
